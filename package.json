  },
  "author": "Greg Domingue",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GregDoming/AlexaNameAnalyzer/issues"
  },
  "homepage": "https://github.com/GregDoming/AlexaNameAnalyzer#readme",
  "dependencies": {
    "ask-sdk-core": "^2.3.0",
    "ask-sdk-dynamodb-persistence-adapter": "^2.3.0",
    "ask-sdk-model": "^1.11.2",
    "aws-sdk": "^2.401.0",
    "child_process": "^1.0.2",
    "dotenv": "^6.2.0",
    "lambda-local": "^1.5.2",
    "rp": "^0.2.0"
  },
  "devDependencies": {
    "adm-zip": "^0.4.13",
    "alexa-skill-test-framework": "^1.2.4",
    "ask-sdk": "^2.3.0",
    "aws-sdk-mock": "^4.3.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cheerio": "^1.0.0-rc.2",
    "dynalite": "^2.2.0",
    "fs": "0.0.1-security",
    "mocha": "^6.0.0",
    "path": "^0.12.7",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "virtual-alexa": "^0.6.14"
  }
